<!DOCTYPE HTML>
<html>
<head>
   <meta charset=UTF-8">
   <title>Manejar imagen</title>
<script type="text/javascript">
function classClick(elemento) {
	if (elemento.getElementsByTagName("td")[0].className=="classOculto") {
		elemento.getElementsByTagName("td")[0].className = "classNormal";
		elemento.getElementsByTagName("td")[1].className = "classNormal";
	} else {
		elemento.getElementsByTagName("td")[0].className = "classOculto";
		elemento.getElementsByTagName("td")[1].className = "classOculto";
	}
}
function classMouseOver(elemento) {
	if (elemento.className!="classOculto") {
		elemento.className="classResaltado";
	}
}
function classMouseOut(elemento) {
	if (elemento.className!="classOculto") {
		elemento.className="classNormal";
	}
}
</script>
<style type="text/css">
.classNormal {
	visibility:visible;}
.classResaltado {
	background-color:yellow;}
.classOculto {
	visibility:hidden;}
table {
	background-color:white;
	width:100%;}
td {
	font-family:"Times New Roman";
	text-align:center;
	font-size:12pt;
	border:4px solid green;}
</style>
</head>
<body>
<table>
<tr class="classNormal" onmouseover="classMouseOver(this);" onmouseout="classMouseOut(this);" onclick="classClick(this);">
	<td>10.30.44</td>
	<td>103</td>
</tr>
<tr class="classNormal" onmouseover="classMouseOver(this);" onmouseout="classMouseOut(this);" onclick="classClick(this);">
	<td>10.30.5</td>
	<td>200</td>
</tr>
<tr class="classNormal" onmouseover="classMouseOver(this);" onmouseout="classMouseOut(this);" onclick="classClick(this);">
	<td>11.3.88</td>
	<td>AGOTADO</td>
</tr>
</table>
</body>
</html>
